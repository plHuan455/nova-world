{
  "name": "3fc-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.5.3",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "script:update-scss": "node scripts/update-scss.js",
    "lint": "run-s lint:script",
    "lint:script": "eslint 'src/**/*.{ts,tsx,js,tsx}'",
    "gen:component": "hygen component new --level .",
    "gen:component:atom": "hygen component new --level atoms",
    "gen:component:molecule": "hygen component new --level molecules",
    "gen:component:organism": "hygen component new --level organisms",
    "gen:component:template": "hygen component new --level templates",
    "gen:component:pages": "hygen component new --level pages",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.9",
    "@storybook/addon-essentials": "^6.1.9",
    "@storybook/addon-links": "^6.1.9",
    "@storybook/addon-storyshots": "^6.1.9",
    "@storybook/node-logger": "^6.1.9",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.9",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@whitespace/storybook-addon-html": "^2.0.1",
    "babel-loader": "8.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "glob": "^7.1.6",
    "hygen": "^6.0.4",
    "node-sass": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "paths.macro": "^3.0.1",
    "react-scripts": "4.0.1",
    "source-map-explorer": "^2.5.1",
    "typescript": "^4.0.3"
  }
}
